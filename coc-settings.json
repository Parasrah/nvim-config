{
  "diagnostic.displayByAle": true,
  "diagnostic.locationlist": true,
  "diagnostic.infoSign": "i",
  "diagnostic.warningSign": "⚠",
  "diagnostic.errorSign": "✗",
  "coc.preferences.formatOnType": false,
  "coc.preferences.formatOnInsertLeave": false,
  "npm.binPath": "npm",
  "snippets.userSnippetsDirectory": "$NVIMCONFIG/coc/ultisnips",
  "coc.preferences.hoverTarget": "float",
  "coc.preferences.useQuickfixForLocations": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "typescript.preferences.importModuleSpecifier": "relative",
  "list.maxPreviewHeight": 30,
  "suggest.autoTrigger": "always",
  "suggest.noselect": true,
  "suggest.enablePreselect": false,
  "suggest.floatEnable": true,
  "codeLens.enable": false,
  "rust-client.disableRustup": true,
  "python.pythonPath": "python3",
  "languageserver": {
    "elmLS": {
      "command": "elm-language-server",
      "filetypes": ["elm"],
      "rootPatterns": ["elm.json"],
      "initializationOptions": {
        "elmPath": "elm",
        "elmTestPath": "elm-test",
        "elmAnalyseTrigger": "never"
      }
    },
    "lua": {
      "command": "lua-lsp",
      "filetypes": ["lua"]
    },
    "fsautocomplete": {
      "command": "dotnet",
      "args": ["/home/brad/.cache/fsautocomplete.netcore/fsautocomplete.dll", "--background-service-enabled"],
      "filetypes": ["fsharp"],
      "initializationOptions": {
        "AutomaticWorkspaceInit": true
      }
    }
  }
}
